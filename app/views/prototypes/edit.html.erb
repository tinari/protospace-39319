<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">プロトタイプ編集</h2>
      <%# 部分テンプレートでフォームを表示する %>
      <%= form_with(model: @prototype, local: true) do |f|  %>
      <%= f.text_field "プロトタイプの名称" :title, id:"prototype_title" %><br>
      <%= f.text_area "キャッチコピー" :catch_copy, class: :form__text, id:"prototype_catch_copy" %><br>
       <%= f.text_area "コンセプト" :concept, class: :form__text, id:"prototype_concept" %><br>
       <%= f.file_field "プロトタイプの画像" :image, id:"prototype_image" %><br>
        <%= f.submit "保存する", class: :form__btn  %> 
      <% end %>
  </div>
</div>