<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">プロトタイプ編集</h2>
      <%# 部分テンプレートでフォームを表示する %>
      <%= form_with(model: @prototype, local: true) do |f|  %>
      <p>プロトタイプの名称</p>
      <%= f.text_field :title, id:"prototype_title" %><br>
      <p>キャッチコピー</p>
      <%= f.text_area :catch_copy, class: :form__text, id:"prototype_catch_copy" %><br>
      <p>コンセプト</p>
       <%= f.text_area :concept, class: :form__text, id:"prototype_concept" %><br>
       <p>プロトタイプの画像</p>
       <%= f.file_field :image, id:"prototype_image" %><br>
        <%= f.submit "保存する", class: :form__btn  %> 
      <% end %>
  </div>
</div>